<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Car Insurrance</title>
    @Styles.Render("~/Content/css")
</head>
<body>
    @RenderBody()

    <div class="card card-body" style="background-color:darkgrey; text-align:center;">
        @ViewBag.Quote
        @ViewBag.AdminData
    </div>
    

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
